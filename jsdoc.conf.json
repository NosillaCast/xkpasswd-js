{
  "opts": {
    "encoding": "utf8",
    "pedantic": true,
    "readme": "./README.md"
  },
  "source": {
    "include": [
      "./src/",
      "./src/lib/"
    ],
    "includePattern": ".+\\.(mjs|js|jsdoc)$"
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  }
}
